x = 2  
y = 1
while x == 2 && x != 2
  while x == 2 && y != 2      
    while x == 2 && y != 2
      while x == 2 && y != 2
        x = 1
        if x == 1 && y != 2
          x = 0
        x = x + 1
        if x == 4 && y != 2
          while x != 3 && y != 2
            x = x - 1
        else
          return 0